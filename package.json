  "main": "src/app.ts",
  "scripts": {
    "start": "npm run dev",
    "dev": "ts-node-dev src/app.ts",
    "prod": "npm run build && node lib/app.js",
    "build": "rm -rf lib && npm run build:types && npm run build:js",
